<frame-haug xmlns="http://www.haug.org/frame"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://www.haug.org/frame frame-haug.xsd ">
    <action-groups>
        <action-classes>
            <action-class name="TestMethod" path="com.frame.info.ActionInfoHolder">
                <action alias="findBookIdByName" name="bookId"/>
                <action alias="insertBook" name="insertBook">
                    <processor path="com.frame.execute.process.DefaultProcessor" type="pre-processor"/>
                    <processor path="com.frame.execute.process.Processor" type="post-processor"/>
                </action>

            </action-class>
            <action-class name="Test2" path="com.frame.execute.process.DefaultProcessor" prefix="findBook" suffix="hahaha">
                <action alias="byId" name="bookId"/>
            </action-class>
        </action-classes>
    </action-groups>

    <action-register>
        <annotation-scan/>
        <action-classes>
            <action-class name="proc" path="com.frame.execute.process.DefaultProcessor">
                <action alias="a,b" name="test1" id="1"/>
                <action alias="c,d" name="test2" id="2"/>
            </action-class>
            <action-class name="" path="com.frame.info.ActionInfoHolder">
                <action alias="e,f" name="test3" id="3"/>
                <action alias="g" name="test4" id="4"/>
                <action alias="h,i,j" name="test5" id="5"/>
            </action-class>
        </action-classes>
        <base-contents>
            <path>com.frame</path>
        </base-contents>
    </action-register>
</frame-haug>