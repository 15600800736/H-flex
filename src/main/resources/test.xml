<frame-haug xmlns="http://www.haug.org/frame"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://www.haug.org/frame frame-haug.xsd ">
    <action-groups>
        <action-classes>
            <action-class name="TestMethod" path="com.frame.info.ActionInfoHolder">
                <action alias="findBookIdByName" name="bookId"/>
                <action alias="insertBook" name="insertBook">
                    <processor path="" type="pre-processor"></processor>
                    <processor path="" type="post-processor"></processor>
                </action>

            </action-class>
            <action-class name="Test2" path="com.frame.process.DefaultProcessor" prefix="findBook" suffix="hahaha">
                <action alias="byId" name="bookId"/>
            </action-class>
        </action-classes>
    </action-groups>

    <action-register>
        <annotation-scan/>
        <action-classes>
            <action-class name="" path="com.frame.process.DefaultProcessor">
                <action alias="" name=""></action>
                <action alias="" name=""></action>
            </action-class>
            <action-class name="" path="com.frame.info.ActionInfoHolder">
                <action alias="" name=""></action>
                <action alias="" name=""></action>
                <action alias="" name=""></action>
            </action-class>
        </action-classes>


        <base-contents>
            <path>com.frame.info</path>
            <path>com.frame.injector</path>
            <path>com.frame.process</path>
        </base-contents>
    </action-register>
</frame-haug>